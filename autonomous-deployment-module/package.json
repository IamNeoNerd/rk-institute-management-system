{
  "name": "autonomous-deployment-module",
  "version": "1.0.0",
  "description": "Enterprise-grade autonomous deployment monitoring and verification system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "config",
    "ui",
    "scripts",
    "README.md",
    "INTEGRATION.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "init": "node scripts/init.js",
    "deploy": "node scripts/deploy.js",
    "monitor": "node scripts/monitor.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "deployment",
    "monitoring",
    "automation",
    "health-checks",
    "vercel",
    "netlify",
    "aws",
    "github",
    "mcp",
    "autonomous",
    "devops",
    "ci-cd"
  ],
  "author": "Autonomous Deployment Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/autonomous-deployment/module.git"
  },
  "bugs": {
    "url": "https://github.com/autonomous-deployment/module/issues"
  },
  "homepage": "https://autonomous-deployment.dev",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "next": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "optionalDependencies": {
